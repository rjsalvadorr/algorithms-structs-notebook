    "cpx": "^1.5.0",
    "http-server": "^0.11.2",
    "jest": "^24.9.0",
    "jsdoc": "^3.6.7",
    "markdown-folder-to-html": "^2.4.0",
    "prettier": "^1.19.1"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
